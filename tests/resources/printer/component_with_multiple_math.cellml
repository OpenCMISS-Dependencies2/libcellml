<?xml version='1.0'?>
<model name="cardiac_AP_minimal"
    xmlns="http://www.cellml.org/cellml/2.0#"
    xmlns:cellml="http://www.cellml.org/cellml/2.0#">
    <component name="environment">
        <variable name="nK_stim" units="dimensionless"/>
        <variable name="stimPeriod" units="second"/>
        <math
            xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci>nK_stim</ci>
                <cn cellml:units="dimensionless">0.3</cn>
            </apply>
        </math>
        <math
            xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci>stimPeriod</ci>
                <cn cellml:units="second">42.6</cn>
            </apply>
        </math>
    </component>
</model>
