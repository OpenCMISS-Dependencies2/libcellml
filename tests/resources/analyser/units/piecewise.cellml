<?xml version='1.0' encoding='UTF-8'?>
<model name="piecewise" xmlns="http://www.cellml.org/cellml/2.0#" xmlns:cellml="http://www.cellml.org/cellml/2.0#">
    <units name="frog"/>
    <units name="rooster"/>
    <units name="dove"/>
    <component name="main">
        <variable name="a" units="dimensionless"/>
        <variable name="b" units="dimensionless"/>
        <variable name="c" units="dimensionless"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci>a</ci>
                <piecewise>
                    <piece>
                        <cn cellml:units="dimensionless">1</cn>
                        <apply>
                            <gt/>
                            <ci>a</ci>
                            <cn cellml:units="dimensionless">3</cn>
                        </apply>
                    </piece>
                    <piece>
                        <cn cellml:units="dimensionless">5</cn>
                        <apply>
                            <gt/>
                            <ci>a</ci>
                            <cn cellml:units="dimensionless">7</cn>
                        </apply>
                    </piece>
                    <otherwise>
                        <cn cellml:units="dimensionless">9</cn>
                    </otherwise>
                </piecewise>
            </apply>
            <apply>
                <eq/>
                <ci>b</ci>
                <piecewise>
                    <piece>
                        <cn cellml:units="second">11</cn>
                        <apply>
                            <gt/>
                            <ci>b</ci>
                            <cn cellml:units="metre">13</cn>
                        </apply>
                    </piece>
                    <piece>
                        <cn cellml:units="second">15</cn>
                        <apply>
                            <gt/>
                            <ci>b</ci>
                            <cn cellml:units="metre">17</cn>
                        </apply>
                    </piece>
                    <otherwise>
                        <cn cellml:units="mole">19</cn>
                    </otherwise>
                </piecewise>
            </apply>
            <apply>
                <eq/>
                <ci>c</ci>
                <piecewise>
                    <piece>
                        <cn cellml:units="frog">21</cn>
                        <apply>
                            <gt/>
                            <ci>c</ci>
                            <cn cellml:units="rooster">23</cn>
                        </apply>
                    </piece>
                    <piece>
                        <cn cellml:units="frog">25</cn>
                        <apply>
                            <gt/>
                            <ci>c</ci>
                            <cn cellml:units="rooster">27</cn>
                        </apply>
                    </piece>
                    <otherwise>
                        <cn cellml:units="dove">29</cn>
                    </otherwise>
                </piecewise>
            </apply>
        </math>
    </component>
</model>
